<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstteam.etcodedecoder">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Basic"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginPass" />
        <activity android:name=".LearnProgram" />
        <activity
            android:name=".Coding"
            android:label="@string/title_activity_coding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Decoding"
            android:label="@string/title_activity_decoding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Registrations" />
        <activity android:name=".FileActivity"></activity>
    </application>

</manifest>